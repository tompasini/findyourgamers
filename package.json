{
  "name": "findyourgamers",
  "version": "1.0.0",
  "scripts": {
    "build": "npm run setup:client && npm run setup:server && npm run build:client",
    "setup:client": "cd findyourgamers.client && npm i",
    "setup:server": "cd findyourgamers.server && npm i",
    "build:client": "npm run --prefix findyourgamers.client build",
    "start": "node findyourgamers.server/index.js"
  },
  "engines": {
    "node": "^14.15.4"
  }
}
